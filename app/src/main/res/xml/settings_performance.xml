<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen
    xmlns:app="http://schemas.android.com/apk/res-auto">

        <PreferenceCategory app:title="Performance Options">
                <CheckBoxPreference
                    app:defaultValue="true"
                    app:key="enable_tunneling"
                    app:title="@string/performance_enable_tunneling" />

                <CheckBoxPreference
                    app:defaultValue="false"
                    app:key="reduced_buffers"
                    app:title="@string/performance_reduced_buffers" />

                <CheckBoxPreference
                    app:defaultValue="false"
                    app:key="exceed_renderer"
                    app:title="@string/performance_exceed_renderer" />
        </PreferenceCategory>

</PreferenceScreen>
